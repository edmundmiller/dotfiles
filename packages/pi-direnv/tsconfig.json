{
  "compilerOptions": {
    "lib": ["ESNext", "ES2022"],
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "types": ["bun", "node"]
  }
}
